{
  "name": "react-isomorph",
  "version": "0.0.0",
  "description": "Isomorphic React Example",
  "main": "app.js",
  "scripts": {
    "watch": "watchify app.js -o public/js/bundle.js -v",
    "browserify": "browserify app.js | uglifyjs > public/js/bundle.js",
    "build": "npm run browserify ",
    "start": "npm install"
  },
  "author": "Ken Wheeler",
  "license": "MIT",
  "dependencies": {
    "express": "~4.22.1",
    "express-handlebars": "~8.0.4",
    "mongoose": "4.2.6",
    "node-jsx": "~0.12.4",
    "react": "~0.14.6",
    "socket.io": "^4.8.3",
    "twitter": "^1.1.0"
  },
  "devDependencies": {
    "browserify": "~17.0.1",
    "nodemon": "~3.1.9",
    "reactify": "~1.1.1",
    "uglify-js": "~3.19.3",
    "watchify": "~4.0.0"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
